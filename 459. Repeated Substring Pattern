class Solution(object):
    def repeatedSubstringPattern(self, s):
        """
        :type s: str
        :rtype: bool
        """
        # n = len(s)
        # for i in range(n - 1):
        #     s0 = s[:i + 1]
        #     if s0 * (n // (i + 1)) == s:
        #         return True
        
        # return False


        return True if s in (s + s)[1:-1] else False
